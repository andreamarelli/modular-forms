{
    "name": "andreamarelli/modular-forms",
    "description": "A package to build modular data collection forms",
    "authors": [
        {
            "name": "Andrea Marelli",
            "email": "andrea.marelli.dev@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "ext-imagick": "*",
        "ext-json": "*",
        "intervention/image": "^2.4",
        "phpoffice/phpspreadsheet": "~1.3",
        "spatie/browsershot": "^3.26"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "AndreaMarelli\\ModularForms\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AndreaMarelli\\ModularForms\\ServiceProvider"
            ]
        }
    }
}
