{
    "name": "andreamarelli/modular-forms",
    "description": "A package to build modular data collection forms",
    "authors": [
        {
            "name": "Andrea Marelli",
            "email": "andrea.marelli.dev@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.2"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0"
    },
    "autoload": {
        "classmap": [
            "src/Controllers"
        ],
        "psr-0": {
            "AndreaMarelli\\ModularForm\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AndreaMarelli\\ModularForm\\ModularFormServiceProvider"
            ]
        }
    }
}
